{
  "name": "Copy Clipboard Table",
  "version": "1.0",
  "description": "Easy way to copy frequently used texts",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "page_action": {
    "default_popup": "index.html"
  }
}